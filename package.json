{
  "name": "the-conditional-orchestra",
  "title": "The Conditional Orhestra",
  "version": "1.1.0",
  "description": "App that takes real-time weather data and converts it to music",
  "author": "Richard Bultitude <richard.bultitude@gmail.com>",
  "contributors": [
    "Richard Bultitude"
  ],
  "repository": {
    "type": "git",
